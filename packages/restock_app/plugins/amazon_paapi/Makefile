.PHONY: doctor
doctor:
	fvm flutter doctor
	
.PHONY: analyze
analyze:
	fvm flutter analyze
	
.PHONY: clean
clean:
	fvm flutter clean
	
.PHONY: pub-get
pub-get:
	fvm flutter pub get
	
.PHONY: build-runner
build-runner:
	fvm flutter pub pub run build_runner build --delete-conflicting-outputs

.PHONY: build-runner-watch
build-runner-watch:
	fvm flutter pub pub run build_runner watch --delete-conflicting-outputs
